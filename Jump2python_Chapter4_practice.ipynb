    "def avg(*args):\n",
    "    result = 0\n",
    "    for i in args:\n",
    "        result = result + i\n",
    "    return result / len(args)\n",
    "\n",
    "print(avg(1,2))\n",
    "print(avg(1,2,3,4,5))"
